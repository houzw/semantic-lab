            <artifactId>jena-fuseki-war</artifactId>
             <type>war</type>
            <version>${fuseki.version}</version>
        </dependency>-->

    </dependencies>
    <build>
        <plugins>
            <!--Plugins for Apache Maven to perform source management activities,
            such as generating Java files from RDF vocabulary sources using Jena's schemagen tool-->
            <plugin>
                <groupId>org.apache.jena</groupId>
                <artifactId>jena-maven-tools</artifactId>
                <version>3.1.0</version>
            </plugin>
        </plugins>
    </build>
