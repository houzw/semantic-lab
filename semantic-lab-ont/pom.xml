            <groupId>org.apache.jena</groupId>
            <artifactId>jena-jdbc-driver-tdb</artifactId>
            <version>${jena.version}</version>
        </dependency>

        <!-- jena-fuseki? -->
        <!--<dependency>
            <groupId>org.apache.jena</groupId>
            <artifactId>jena-fuseki</artifactId>
            &lt;!&ndash;<type>pom</type>&ndash;&gt;
            <version>${fuseki.version}</version>
        </dependency>
        <dependency>
